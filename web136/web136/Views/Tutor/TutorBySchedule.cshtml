@{
    ViewBag.Title = "TutorBySchedule";
    //var id = ViewBag.Id;
}

<h2>TutorBySchedule</h2>
<div id="tutorListContent">
    <table>
        <thead>
            <tr>
                <td>First</td>
                <td>Last</td>
                <td>Type</td>
            </tr>
        </thead>
        <tbody data-bind="foreach: viewModel">
            <tr>
                <td data-bind="text: first"></td>
                <td data-bind="text: last"></td>
                <td data-bind="text: type"></td>
            </tr>
        </tbody>
    </table>
</div>

<!-- ajax script call HERE-->
<script type="text/javascript">
    $(document).ready(function () {
        require(['ViewModels/TutorViewModel'], function (tutorViewModel) {
            var tutorViewModelObj = new tutorViewModel();
            tutorViewModelObj.GetListBySchedule(@ViewBag.Id);
        });
    });
</script>

